<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
	<dubbo:application name="dubbo-demo-provider-container-app" />
	<dubbo:registry address="zookeeper://zkserver1:2181" />
	<dubbo:provider retries="0" delay="-1"  filter="appNameAwareFilter" />
	<dubbo:protocol name="dubbo" port="20880" />
    <dubbo:service interface="study.learn.dubbo.api.DemoService" ref="demoServiceImpl" />
    <dubbo:service interface="study.learn.dubbo.api.DemoService2" ref="demoService2Impl" />
</beans>